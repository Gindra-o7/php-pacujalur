{
  "name": "pacujalur/backend-api",
  "description": "Backend API untuk aplikasi Pacu Jalur",
  "type": "project",
  "require": {
    "php": ">=8.0",
    "firebase/php-jwt": "^6.11"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Config\\": "configs/",
      "Middlewares\\": "middlewares/",
      "Routes\\": "routes/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "scripts": {
    "test": "phpunit",
    "serve": "php -S localhost:8000 -t public"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
